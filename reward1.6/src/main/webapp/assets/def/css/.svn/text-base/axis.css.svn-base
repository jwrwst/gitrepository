
div,form,input,textarea,p,ul,li,dl,dt,dd,h1,table,h2 {
	padding:0;
	margin:0;
	list-style: none;
}


.content .wrapper {
	position: relative;
	margin: 0 auto;
/* 	top:10px; */
}

.content .main .title {
	position: absolute;
	padding-left: 35px;
	top: -18px;
	color: #919191;
	font-size: 13px;
	z-index:3;
}
.content .main .title .mui-icon{
	margin-right:15px;
}
.cls .mui-icon{
	margin-right:10px;
	z-index: 3;
	position: relative
}
 .cls .icon-star,.ranklist .icon-star{
   	margin-right:0px!important;
   	font-size:13px!important;
}
.cls .intro-text{
	color:#645D5D;
	font-size:14px;
	padding-top:-10px;
}

.cls .intro-time{
	float:right;
	font-size:13px;
	padding-right:5px;
	
}

.clsdt{
	padding:0px!important;
	border:0px!important;
	margin-left:35px;
}
.clsdt span{
	font-size:13px;
}

.clsdt .mui-icon{
	margin-right:15px;
	z-index: 3;
	position: relative;
	color:#919191;
}
.clsdt .intro-text{
	color:#919191;
	font-size:13px;
	
}

.cls p{
	font-size:13px;
	margin-bottom:0px!important;
}


.content .main .year {
	position: relative;
/* 	z-index: 100; */
}


.content .main .year .list{
/* 	margin:10px 0; */
	position:relative;
	overflow:hidden;
/* 	padding-bottom:20px; */
/* 	-webkit-transition:height 1s cubic-bezier(0.025,0.025,0.000,1.115),opacity 1s; */
/* 	-moz-transition:height 1s cubic-bezier(0.025,0.025,0.000,1.115),opacity 1s; */
/* 	-ms-transition:height 1s cubic-bezier(0.025,0.025,0.000,1.115),opacity 1s; */
/* 	-o-transition:height 1s cubic-bezier(0.025,0.025,0.000,1.115),opacity 1s; */
/* 	transition:height 1s cubic-bezier(0.025,0.025,0.000,1.115),opacity 1s */
}
.content .main .year .list ul {
	bottom: 0;
	top:30px;
}
.content .main .year .list ul li {
	padding: 10px 0 ;
	color: #a1a4b8;
	border:1px solid #E4E2E2;
}

.cls {
	zoom: 1;
	margin-top:5px;
	background-color: #ffffff;
}
.cls:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}
.content .main .year .list ul li.highlight .date, .content .main .year .list ul li.highlight .intro {
	color: #ec6a13;
}
.content .main .year .list ul li .date,.content .main .year .list ul li .version{
	float:left;
	display:block;
	clear:left;
	width:200px;
	line-height:20px;
	text-align:right
}
.content .main .year .list ul li .date{
	font-size:13px;
	line-height:32px;
	color:#bec1d5
}
.content .main .year .list ul li .intro, .content .main .year .list ul li .more {
	float: left;
	display: block;
	margin-left: 29px;
	line-height: 20px;
}

.content .main .year .list ul li .more {
	margin: 0px 20px 0 59px;
   	position: relative;
	width: 80%;
	left: 0px;
	overflow: hidden;
	word-wrap: break-word;
	font-size: 0.8em;
}

.content .main .year .list ul li .intro {
	font-size: 13px;
	line-height: 32px;
}
.content .wrapper:first-child .main .year.close h2 i{
	transform:rotate(-90deg);
	-webkit-transform:rotate(-90deg);
	-moz-transform:rotate(-90deg);
	-ms-transform:rotate(-90deg);
	-o-transform:rotate(-90deg)
}
.content .wrapper:first-child .main .year.close .list{
	opacity:0;
	height:0!important;
}

.content .main .year h2 {
	height: 40px;
	width: 170px;
	padding-right: 30px;
	font-size: 13px;
	line-height: 40px;
	text-align: right;
}
.content .main .year h2 a {
	color: #58a6fb;
}

.content hr{
	width:5px;
	height:100%;
	background-color: rgba(247, 243, 243, 0.82);
	position: absolute;
	left:37px;
	z-index:2;
	top:-9px;
	border:0;
}
