<?xml version="1.0" encoding="UTF-8"?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3g.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee web-jsptaglibrary_2_0.xsd"
	version="2.0">

	<description>plf taglib!</description>
	<display-name>Taglib</display-name>
	<tlib-version>1.0</tlib-version>
	<short-name>plf</short-name>
	<uri>http://www.platform.com</uri>

	<tag>
		<name>select</name>
		<tag-class>com.platform.rp.common.tags.SelectTag</tag-class>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>select的属性name和id</description>
		</attribute>
		<attribute>
			<name>classify</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>代码字典分类值</description>
		</attribute>
		<attribute>
			<name>fixed</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>需要选中代码字典</description>
		</attribute>
		<attribute>
			<name>style</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>下拉框的行内样式属性style</description>
		</attribute>
		<attribute>
			<name>clazz</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>下拉框的样式class</description>
		</attribute>
		<attribute>
			<name>excludes</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>除此之外的选项</description>
		</attribute>
		<body-content>JSP</body-content>
	</tag>
	
	<tag>
		<name>show</name>
		<tag-class>com.platform.rp.common.tags.ShowNiceTag</tag-class>
		<attribute>
			<name>choose</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>被分割的字符串</description>
		</attribute>
		<attribute>
			<name>token</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description>分隔符</description>
		</attribute>
		<attribute>
			<name>fixed</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>需要显示的字符的索引</description>
		</attribute>
		<attribute>
			<name>from</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>代码字典类型名称</description>
		</attribute>
		<body-content>JSP</body-content>
	</tag>
	
	<tag>
		<name>radio</name>
		<tag-class>com.platform.rp.common.tags.RadioTag</tag-class>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>radio的属性name和id</description>
		</attribute>
		<attribute>
			<name>classify</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>代码字典分类值</description>
		</attribute>
		<attribute>
			<name>fixed</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>需要选中代码字典</description>
		</attribute>
		<attribute>
			<name>excludes</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>除此之外的选项</description>
		</attribute>
		<attribute>
			<name>wraper</name>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>外层标签。如： li / a </description>
		</attribute>
		<body-content>JSP</body-content>
	</tag>
	
</taglib>