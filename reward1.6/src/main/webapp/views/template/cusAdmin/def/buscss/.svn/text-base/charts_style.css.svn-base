}
.charts_tabs{
	height: 30px;
	margin:20px 0 15px;
	width: auto !important;
	display: inline-block;
	overflow: hidden;
	border-radius: 5px !important;
	border: 1px solid lightgray;
}
.charts_tabs a{
	padding: 5px 30px;
	display: inline-block;
	border-right:1px solid lightgray;
	float:left;
}
.charts_tabs a:nth-child(3){
	border:none;
}
.charts_tabs a:hover{
	background: #3d3d3d;
	color: #fff;
	border: none;
	margin-left: 0;
	text-decoration:none;
}
.charts_tabs a.active{
	background: #3d3d3d;
	color: #fff;
	border: none;
	margin-right: 0;
}
