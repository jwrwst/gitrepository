pan>								<br /> <span class="control-title">已选店数:</span> <span									id="selectStoreVal"></span>							</div>						</div>					</div> -->										<!-- 								<div class="row-fluid">									<div class="span6 ">										<div class="control-group">											<span class="control-title" for="firstName">日结时间：</span>											<input id="dateAddTime" class="m-wrap m-ctrl-small timepicker-24" type="text">														</div>									</div>								</div> -->					<div class="row-fluid" style=' border-bottom: 1px solid #eee;'>					<div style='margin-left:75px; min-height:100px;'>						<div class="span8">							<div class="control-group">								<span class="control-title" for="firstName">所在地区：</span>								<select class="mui-btn mui-btn-block auto-select-province" id="provinceId" cid = "city"										style="width: auto; background: transparent; padding-right: 20px; text-align: center;">								</select>								<select class="mui-btn mui-btn-block auto-select-city" id="cityId" cid = "area"										style="width: auto; background: transparent; padding-right: 20px; text-align: center;">								</select>								<select class="mui-btn mui-btn-block auto-select-area" id="areaId" name="areaType"										style="width: auto; background: transparent; padding-right: 20px; text-align: center;">								</select>							</div>						<!-- </div>					</div>					<div class="row-fluid">						<div class="span8" style='float:left;'> -->							<div class="control-group">								<span class="control-title" for="firstName">选择店铺：</span>								<div id="selectStore" class="select_store" style='margin-left:75px;'></div>							</div>						</div>					<!-- </div>					<div class="row-fluid"> -->						<div class="span4" style='float:right;'>							<div class="control-group">								<input type="button" class="chart_button_1" value="确定" id="seacher">							</div>						</div>						</div>					</div></div>					<div class="row-fluid" style='margin-to