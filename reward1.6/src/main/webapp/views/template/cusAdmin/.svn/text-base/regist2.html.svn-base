[#include "model/header-unmenu.html"]
<meta charset="utf-8" />
	<style type='text/css'>
body {
	overflow-x: hidden;
}

.file-box {
	position: relative;
	width: 100%;
}
.file-box span{width: 100%; font-size: 30px; font-weight: bold; min-height: 200px; display: block; }
.txt {
	height: 22px;
	border: 1px solid #cdcdcd;
	width: 180px;
}

.file {
	/* position: absolute;
	top: 0; */
	min-heihgt: 100px;
	filter: alpha(opacity : 0);
	opacity: 1;
	width: 100%;
	display: block;
	min-heihgt: 100px;
}
.form-row-seperated .control-label{ width:120px !important;}
.form-row-seperated .control-group{ border:none;}
.form-row-seperated .controls{ margin-left:130px;}
.form .form-actions{ padding-left:130px;}
</style>


<div class="row-fluid">

	<div class="span12">
		<div class="portlet-body form"
			 style='width:60%; display:block; min-width:800px; overflow:hidden; margin:45px 20%;'>
			<form action="#" class="form-horizontal form-row-seperated">
				<div class='result' id="result1" style="display:none;">
				<input type="hidden" name="filePath" id="filePath">
				<div class="control-group" style='height:auto;'>

						<p
							style='height: 40px; text-align: center; font-size: 20px; font-weight: bold; line-height: 40px;'>提交成功！正在进行审核…</p>
						<p style='text-align: center;'>

							提交24小时后，反馈审核结果。请耐心等待
						</p>

					</div>
					<h4 class="form-section merchantsdis" style=''></h4>
					<div class="control-group">

						<label class="control-label">品牌名称</label>

						<div class="controls">

							<input type="text" name="orgName" readonly='true' class="m-wrap span12"
								placeholder="请输入商户名称，如：众赏">

						</div>

					</div>
					<div class="control-group">

						<label class="control-label" for="orgName">公司名称</label>

						<div class="controls">

							<input type="text" name="merName" readonly='true' class="m-wrap span12"
								placeholder="请输入营业执照上的公司名称">

						</div>

					</div>
					
					<div class="control-group orgManagerDis">

						<label class="control-label" for="store">所属行业</label>

						<div class="controls">
							<div class="span12">
								<span class="mui-btn mui-btn-block"
									style=" margin-right: 30px; line-height: 34px;" id="showCityPicker">
									</span>
							</div>
						</div>

					</div>
					<div class="control-group orgManagerDis">
						<label class="control-label">所在地区</label>
						<div class="controls">
							<span class="mui-btn mui-btn-block auto-select-province"
								name="provinceId" id="showCityPicker3" data-id="city"
								style=" margin-right:30px; line-height:34px;">
							</span> 
						</div>
					</div>

					<div class="control-group orgManagerDis">
					
					<label class="control-label">详细地址</label>

						<div class="controls">

							<input type="text" class="m-wrap span12" readonly='true'
								placeholder='请填写详细地址，不必重复填写省市区信息' name="address">

						</div>

					</div>
					
					<div class="control-group">
					<label class="control-label">营业执照</label>
					<div class='controls'>
						<div class="file-box">
							<!-- <input type='text' name='textfield' id='textfield' class='txt' /> -->
							<span class="header" style='background: url(media/image/logo.png) left center no-repeat; background-size:auto 100%; ' /></span>
							<!-- <input type="file" name="fileField" class="file"
								style='display: block;' /> -->
							<!-- <input type="submit" name="submit" class="btn" value="上传" /> -->
						</div>
						</div>
					</div>


					<div class="form-actions">

						<button type="button" class="btn blue" onclick="updateInfo()">
							修改信息
						</button>
                        <button type="button" class="btn gray" onclick="window.history.go(-1)">
							取消
						</button>

					</div>
				</div>
				<div class='result' id="result3" style='display:none;'>
					<div class="control-group" style='height:auto;'>

						<p
							style='height: 40px; text-align: center; font-size: 20px; font-weight: bold; line-height: 40px;'>很抱歉，您未通过审核！</p>
						<p style='text-align: center;'>

							原因：<span id="remark">营业执照不清晰，请重新上传</span><br /> 如您对审核有疑问，请联系客服<b style='color:#e02222;'>4000-685-456</b>
						</p>

					</div>


					<div class="form-actions">

						<button type="button" class="btn blue" onclick="updateInfo()">修改并重新提交审核</button>
 						<button type="button" class="btn gray" onclick="window.history.go(-1)">
							取消
						</button>

					</div>
				</div>
			</form>
		</div>


	</div>

</div>

[#include "model/footer-unmenu.html"]
</body>


	
	<script src="def/cus_addr.js" type="text/javascript"></script> 
	<script type="text/javascript" src="def/regist2.js?v=3123213"></script>
<script type="text/javascript">

var status = window.sessionStorage.getItem("status");
//alert(status)
if(status == null || status == "" || status == "null"){
	status = 1;
}
//alert(status)
document.getElementById("result"+status).style.display="";


</script>
</html>



