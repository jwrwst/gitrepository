<link href="${basepath}/assets/def/css/tips.css?v=9432424234234384aabb" rel="stylesheet" />
<style>
	body{
		background-color: #F6F1EE;
		width: 100%;
		height:100%;
	}
	.header .img {
	    background: url("/assets/def/images/tenant.png") no-repeat center center;
	    width: 100%;
	    min-height: 150px;
	    display: block;
	    background-size: 150px auto;
	}

	.mui-table-view{margin-bottom: 45px}
	.mui-table-view li{width:100%; display:block;}
	.mui-table-view li a{position:relative; padding-left:20px;}
	.mui-table-view li a.iconfont{font-size:14px !important;}
	.mui-table-view li.disabled{text-decoration:line-through; color:#b3b3b3;}
	.mui-table-view li.disabled span{ position:absolute; right:0; width: 85px;}
	.icon-storemanage:before{ color:#EA4F16;}
	.mui-table-view li span{ position:absolute; right:0; width: 35px;}
.mui-table-view li .icon-storemanage:before	{font-size: 20px; color:#fdd79f;}
.mui-icon-left-nav:before{ color:#EA4F16;}
.icon-quyu:before{
content:url("/assets/def/images/quyu.png");
position:relative;
bottom:0;
}
</style>
</head>
<body>
	<header class="mui-bar mui-bar-nav">
	<a class="mui-btn-nav mui-pull-left" style='color:#b3b3b3;'><span class="mui-icon mui-icon-left-nav"></span>北京区域</a>
		<span style='position:relative; width:130px; display:block; margin:0 auto;'> <span
			class="mui-icon iconfont icon-up"
			style="position: absolute; left: 115px; top: 6px; color: #cccccc; font-size: 12px !important;"></span>
			<select class="mui-btn mui-btn-block mui-pull-right"
			style="max-width: 105px; background: transparent; display:block; position: absolute; left: 0;"
			name="dateType" id="merchantsInfo">
		</select>
		</span><p style="float:right; font-size: 14px;line-height: 44px;color: #EA4F16;" id="userInfo">账户与安全</p>
	</header>
	<div style="margin-top: 50px;position: relative;" >
		<!-- tab1 start -->
		<div id="tab1" >
		<div style="width:100%; position:fixed; height: 45px; top:46px; background:#F6F1EE; z-index:999;">
			<div style="margin:5px 10px 0; height: 50px;">
					<span id="addStore"
									class="mui-icon mui-icon-plusempty" style="width:10%; float:left; color: #EA4F16; height:50px; line-height:50px;"></span>
				<input type="text" class="" placeholder="搜索" name="storeaddress" maxlength="100"  id="searchVal"
				style="padding-left: 15%;text-align:left;margin-top:2px;border: none;width: 90%; float: left;">
				<a class="mui-icon mui-icon-search" style="position: absolute;left:15%;line-height: 45px;color: #999" id="search"></a>
			</div>	
			
			</div>	
				 <div style="clear: both;overflow: auto; margin-top:100px;">
					<ul class="mui-table-view">
						<li class="mui-table-view-cell"><span class="mui-icon iconfont icon-delete"></span><a class=' mui-icon iconfont icon-storemanage' > 众赏旗舰店2 </a></li>
						<li class="mui-table-view-cell"><span class="mui-icon iconfont icon-delete"></span><a class=' mui-icon iconfont icon-quyu' > 众赏旗舰店3 </a></li>
						<li class="mui-table-view-cell"><span class="mui-icon iconfont icon-delete"></span><a class=' mui-icon iconfont icon-storemanage' > 众赏旗舰店4 </a></li>
						<li class="mui-table-view-cell disabled"><span>已停用</span><a
							class="mui-navigate-right"> 众赏旗舰店3</a></li>
					</ul>
				</div>	
			</div>		
		<!-- tab1 end -->
		
	</div>	
	<nav class="mui-bar mui-bar-tab store-menu" style="z-index:9999999999">
			<a class="mui-tab-item" href="javascript:void(0)" id="dynamic" data-menu="dynamic">
				<span class="mui-icon iconfont icon-store-list" data-class="icon-time"></span>
				<span class="mui-tab-label">门店列表</span>
			</a>
			<a class="mui-tab-item" href="javascript:void(0)" id="range" data-menu="range">
				<span class="mui-icon iconfont icon-range" data-class="icon-range"></span>
				<span class="mui-tab-label">排行</span>
			</a>
			<a class="mui-tab-item" href="javascript:void(0)" id="statistics" data-menu="statistics">
				<span class="mui-icon iconfont icon-serverteen" data-class="icon-serverteen"></span>
				<span class="mui-tab-label">统计</span>
			</a>
			<a class="mui-tab-item" href="#setting_menu" data-id="setting_menu" data-menu="setting_menu">
				<span class="mui-icon iconfont icon-storemanage" data-class="icon-setting"></span>
				<span class="mui-tab-label">商户信息</span>
			</a>
		</nav>
	

