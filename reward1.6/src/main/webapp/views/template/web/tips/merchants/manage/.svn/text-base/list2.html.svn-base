background:url("/assets/def/images/quyu.png") left center no-repeat;
padding-left:20px;
background-size:18px;
}
.newarea ul.nobg{ margin-left:-20px;}
.newarea ul.nobg li{background: none !important; padding-left:0; display:block;}
.newarea ul.nobg .icon-storemanage:before{font-size: 18px; margin-right:5px; color:#fdd79f !important;}
