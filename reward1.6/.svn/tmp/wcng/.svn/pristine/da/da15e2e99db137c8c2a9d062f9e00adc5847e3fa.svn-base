<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.platform.rp.services.store.core.dao.BsStoreAuthDAO">

    <insert id="save" parameterType="com.platform.rp.services.store.core.dao.entity.BsStoreAuthEntity" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO bs_store_auth (auth_name,auth_code,group_code)
        VALUES (#{authName}, #{authCode}, #{groupCode})

    </insert>
    
    <select id="getList" resultType="com.platform.rp.services.store.core.dao.entity.BsStoreAuthEntity" parameterType="string">
         select s.auth_name authName,s.auth_code authCode,s.group_code groupCode from bs_store_auth s where s.group_code = #{groupCode}       
    </select>
  
    

</mapper>