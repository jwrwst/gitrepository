package com.platform.rp.services.organize.external.vo;

import java.io.Serializable;
import java.math.BigDecimal;
import java.util.List;

import com.platform.rp.services.organize.core.dao.entity.BsMerchantsExtRewardEntity;
import com.platform.rp.services.organize.core.dao.entity.BsMerchantsRewardStoreEntity;


/**
 * The persistent class for the bs_platform_reward database table.
 * 
 */
public class BsMerchantsVo implements Serializable {
	private static final long serialVersionUID = 1L;
	private String orgCode;
	
	private String allotPlan;
	
	private String wish;
	
	private BigDecimal rewardMoney;
	
	private int isUpdateMoney;
	
	private List<BsMerchantsExtRewardEntity> merchantsExtReward;
	
	private List<BsMerchantsRewardStoreEntity> merchantsRewardStoreEntity;
	 
	

	public BsMerchantsVo() {
	}

	
	public String getWish() {
		return wish;
	}


	public void setWish(String wish) {
		this.wish = wish;
	}


	public BigDecimal getRewardMoney() {
		return rewardMoney;
	}


	public void setRewardMoney(BigDecimal rewardMoney) {
		this.rewardMoney = rewardMoney;
	}


	public int getIsUpdateMoney() {
		return isUpdateMoney;
	}


	public void setIsUpdateMoney(int isUpdateMoney) {
		this.isUpdateMoney = isUpdateMoney;
	}


	public String getAllotPlan() {
		return allotPlan;
	}



	public void setAllotPlan(String allotPlan) {
		this.allotPlan = allotPlan;
	}



	public String getOrgCode() {
		return orgCode;
	}



	public void setOrgCode(String orgCode) {
		this.orgCode = orgCode;
	}



	public List<BsMerchantsExtRewardEntity> getMerchantsExtReward() {
		return merchantsExtReward;
	}



	public void setMerchantsExtReward(
			List<BsMerchantsExtRewardEntity> merchantsExtReward) {
		this.merchantsExtReward = merchantsExtReward;
	}



	public List<BsMerchantsRewardStoreEntity> getMerchantsRewardStoreEntity() {
		return merchantsRewardStoreEntity;
	}



	public void setMerchantsRewardStoreEntity(
			List<BsMerchantsRewardStoreEntity> merchantsRewardStoreEntity) {
		this.merchantsRewardStoreEntity = merchantsRewardStoreEntity;
	}

	

	

	

}