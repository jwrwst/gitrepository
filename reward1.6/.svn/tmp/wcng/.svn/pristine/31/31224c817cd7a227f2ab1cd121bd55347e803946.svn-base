		jQuery(document).ready(function() {     
		  App.init();
		  Login.init();
		});		function toRefindPassword(){			window.location.href="wap-refindpassword.xhtml";		}		function regist(){			window.location.href="wap-regist.xhtml?type=3";		}	</script>

