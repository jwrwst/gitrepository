<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.platform.rp.services.organize.core.dao.BsPlatformInfoDAO">
    
    <resultMap type="com.platform.rp.services.organize.core.dao.entity.BsPlatformInfoEntity" id="bsPlatformInfoEntity">
		<result column="code" property="code" />
       	<result column="reward_money" property="rewardMoney" />
		<result column="isUpdateMoney" property="isUpdateMoney" />
		<result column="allot_plan" property="allotPlan" />		
		<result column="update_user" property="updateUser" />
		<result column="update_time" property="updateTime" />
    </resultMap>
    
	<select id="getInfo" resultMap="bsPlatformInfoEntity" parameterType="string">
	    select * from bs_platform_info where code = #{code}
	</select>

</mapper>