<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.platform.rp.services.sys.core.dao.SysOprateInfoDAO">
	
    <resultMap type="com.platform.rp.services.sys.core.dao.entity.SysOprateInfoEntity" id="sysOprateInfoMap">
        <result column="id" property="id" />
		<result column="module" property="module" />
		<result column="operate_log" property="operateLog" />
		<result column="type" property="type" />
		<result column="operator" property="operator" />
		<result column="timestamp" property="timestamp" />
    </resultMap>
	    
	<insert id="save" parameterType="com.platform.rp.services.sys.core.dao.entity.SysOprateInfoEntity" useGeneratedKeys="true" keyProperty="id">
	    insert into sys_operate_info(module,operate_log,type,operator,timestamp)
	    values(#{module},#{operateLog},#{type},#{operator},CURRENT_TIMESTAMP())
	</insert>
	
</mapper>